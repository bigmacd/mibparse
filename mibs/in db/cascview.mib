CASCADEVIEW-MIB DEFINITIONS ::= BEGIN

IMPORTS
    enterprises, IpAddress, Counter,
    Gauge, TimeTicks
        FROM RFC1155-SMI
    DisplayString, ifInErrors, egpNeighAddr
        FROM RFC1213-MIB
    OBJECT-TYPE
        FROM RFC-1212
    TRAP-TYPE
        FROM RFC-1215
