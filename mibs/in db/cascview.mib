    ::= { cascview 17 }

cvNodeBillingStatusStr OBJECT-TYPE
    SYNTAX  DisplayString
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
        "A textual description of the node's billing status."
    ::= { cascview 18 }

cvNodeBillingAPAddressStr OBJECT-TYPE
    SYNTAX  DisplayString
    ACCESS  read-only
