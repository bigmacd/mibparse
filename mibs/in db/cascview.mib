	FROM CASCADE-MPT-MIB
    diskDirectory, diskPercentageUsed, diskSeverity, 
    thresholdValue, thresholdSeverity
        FROM CASCADE-ACCTSERVER-MIB
    cascade, cascview, pportSlotId, pportId, nodeFanIndex, nodeSwFilename, 
        nodePrFilename, lportCongestRate, nodeDiagNonFatalTime,
        nodeDiagNonFatalErrMajor, nodeDiagNonFatalErrMinor, nodeConsoleIndex,
        nodeUserName, nodeConsoleUptime, cardPhysicalSlotId,
        cardDiagNonFatalTime, cardDiagNonFatalErrMajor, nodeDiagNonFatalStr,
        lportErrorThreshold, lportCongestRateThresh,
	    chanSlotId, chanPortId, chanId, 
        cardDiagNonFatalErrMinor, cktFailPort, cktFailNode,
        pportDS0FarendDS0InLpbk, nodeFileTransferRequest, pportSetClkBkup
        svcAtmConfigQ93bLastCauseTx, svcAtmConfigQ93bLastCauseRx,
	    cktSvcCalledParty, cktSvcCallingParty, pportPMTcaId, 
        lportIfIndex, cktNdcIfIndex, cktNdcSrcDlci, lportSevereCongestStatus,
        cktNdcInDiscardClp0Cells, cktNdcInDiscardClp0CellThresh,
        cktNdcInDiscardClp01Cells, cktNdcInDiscardClp01CellThresh,
	    cktLeafSrcIfIndex, cktLeafAtmVPI, cktLeafAtmVCI, cktLeafEndpointIndex,
        cktLeafOperStatus, cktLeafFailReason, cktLeafFailNode, cktLeafFailPort,
        atmacctControl, atmacctASAddressPri, atmacctASAddressSec,
        atmacctASControl, atmacctSwASCommsFailures, atmacctUsageRecCrFailures,
        atmacctLportSvcControl, atmacctLportIfIndex, 
        lportSlotId, lportPportId, lportId, atmacctOperState, atmacctASCommsState, 
        svcAddrAddress, svcPrefixPrefix, svcNodePrefixPrefix, 
        isdnCallerIDAddr, dvcCktGrpDialedE164Addr, 
        nodeBulkStatsCollectorAddress, nodeBulkSwAPCommsFailures, 
        nodeAdminStatus, nodeOperatingStatus, nodeNtpOffset,
	lportDataRate, lportVAvailBw,
        cardLogicalSlotId, subcardPhysicalSubSlotId,
        subcardOperType, subcardOperStatus

    FROM CASCADE-MIB;

--  This MIB module uses the extended OBJECT-TYPE macro as defined in RFC 1212

--  Title

